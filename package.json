{
  "name": "lite-frontend",
  "version": "1.0.0",
  "description": "Combined repo for LITE exporter and caseworker frontends",
  "private": true,
  "scripts": {
    "build_exporter_css": "parcel build exporter/assets/scss/styles.scss --out-dir exporter/assets/built --public-url /assets/",
    "build_caseworker_css": "parcel build caseworker/assets/scss/styles.scss  --out-dir caseworker/assets/built --public-url /assets/",
    "build": "parcel build ./caseworker/bundle.js ./exporter/bundle.js",
    "watch": "parcel watch ./caseworker/bundle.js ./exporter/bundle.js"
  },
  "engines": {
    "node": ">=12.16.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uktrade/lite-frontend.git"
  },
  "author": "DIT",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uktrade/lite-frontend/issues"
  },
  "homepage": "https://github.com/uktrade/lite-frontend#readme",
  "dependencies": {
    "govuk-frontend": "^3.7.0"
  },
  "devDependencies": {
    "@popperjs/core": "^2.4.4",
    "accessible-autocomplete": "^2.0.3",
    "govuk-frontend": "^3.9.0",
    "highlight-within-textarea": "^2.0.5",
    "jquery": "^3.5.1",
    "lightpick": "^1.6.2",
    "mark.js": "^8.11.1",
    "parcel-bundler": "^1.12.4",
    "sass-lint": "^1.13.1",
    "sass-loader": "^10.0.2",
    "sortablejs": "^1.11.2-alpha.3",
    "tippy.js": "^6.2.6",
    "tokenfield": "^1.4.4",
    "tributejs": "^5.1.3"
  }
}
